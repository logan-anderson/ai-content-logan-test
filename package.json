{
  "name": "ai-content",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "AI suggestions for your content",
  "author": "Logan Anderson",
  "license": "MIT",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app",
    "ai",
    "chatGPT"
  ],
  "scripts": {
    "types": "tsc --emitDeclarationOnly",
    "build": "tsup",
    "build:action": "tsup src/action.ts",
    "build:lambdaFunction": "tsup src/lambdaFunction.ts",
    "prettier": "prettier --write .",
    "watch": "tsup --watch --onSuccess \"node dist/server.js\"",
    "start": "node dist/server.js",
    "test-watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@netlify/functions": "^1.6.0",
    "@probot/adapter-aws-lambda-serverless": "^3.0.3",
    "@probot/adapter-github-actions": "^3.1.3",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.1",
    "openai": "^3.3.0",
    "probot": "^12.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "jest": "^29.0.0",
    "nock": "^13.0.5",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "smee-client": "^1.2.2",
    "ts-jest": "^29.0.0",
    "tsup": "^7.0.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "18.x"
  }
}
